{
    "name": "Dog Breed Classification",
    "build": {
      "dockerfile": "../Dockerfile",
      "context": ".."
    },
    "customizations": {
      "vscode": {
        "settings": {
          "python.pythonPath": "/usr/local/bin/python",
          "terminal.integrated.shell.linux": "/bin/bash"
        },
        "extensions": [
          "ms-python.python",
          "ms-azuretools.vscode-docker"
        ]
      }
    },
    "forwardPorts": [8888],
    "postCreateCommand": "pip install .",
    "remoteUser": "root"
  }
  